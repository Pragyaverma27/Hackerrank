#include<iostream>
#include<cstdio>
#include<cmath>
#include<vector>
#include <bits/stdc++.h>

using namespace std;

int main()
{
    int i,n,sum=0;
    cin>>n;
    vector<int> arr(n);
    
    for(i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    
    for(i=0;i<n;i++)
    {
        sum=sum+arr[i];
    }
    cout<<sum;;
    return 0;
}
